# .xprofile
# sourced by .xinitrc
# common items to start with window manager

export XDG_CONFIG_HOME="$HOME/.config"

killall -q conky &
killall -q polybar &
killall -q i3status &
killall -q sxhkd &

numlockx on &

xset s 3600 3600 &
xset b 33
xset dpms 3600 3700 4000

# background
xsetroot -solid darkblue &
# fbsetbg -c -r "$HOME/wallpaper/" &

PATH=$PATH:~/conky:./


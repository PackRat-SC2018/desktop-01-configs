! Xft settings ---------------------------------------------------------
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! special
*.foreground:   #DCDCDC
*.background:   #131414
*.cursorColor:  #CCCCCC

! black
*.color0:       #1c1d1e
*.color8:       #373b41

! red
*.color1:       #c13333
*.color9:       #cc6666

! green
*.color2:       #4f9440
*.color10:      #b5bd68

! yellow
*.color3:       #deb940
*.color11:      #f0c674

! blue
*.color4:       #17578b
*.color12:      #81a2be

! magenta
*.color5:       #85678f
*.color13:      #b294bb

! cyan
*.color6:       #5e8d87
*.color14:      #8abeb7

! white
*.color7:       #C0C1C2
*.color15:      #E1E2E3

! URxvt -----------------------------------------------------------------------
URxvt*termName:          rxvt-256color
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false
URxvt*geometry:          100x34
URxvt*transparent:       false
URxvt*depth:           32
! URxvt.depth:             24
URxvt.background:        [90]#131314
!URxvt.iso14755:          false
URxvt*internalBorder:    6
! URxvt*externalBorder:    1

!URxvt.font:              -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!URxct.boldFont:          -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
URxvt.font:              xft:Code New Roman:Regular:size=12:antialias=true:autohint=true
URxvt.boldFont:          xft:Code New Roman:Regular:size=12:antialias=true:autohint=true
URxvt.letterSpace: 1
URxvt*antialias:              true
URxvt*autohint:               true

URxvt.perl-ext-common: default,matcher
!URxvt.clipboard.autocopy: true
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste
!URxvt.matcher.button: 1
URxvt.urlLauncher:      firefox
URxvt.tabbed.tabbar-fg: 14
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    13
URxvt.tabbed.tab-bg:    0

! XTerminals ------------------------------------------------------------
UXterm*termName:          xterm-256color
UXTerm*geometry:          100x34
!UXTerm*background:       #101012
!UXTerm*foreground:       #E5E5E5
!UXTerm*cursorColor:      #E5E5E5
!UXTerm*faceName:          Monospace:regular:size=11:antialias=true:autohint=true
UXTerm*faceName:          Office Code Pro:regular:size=12:antialias=true:autohint=true
!UXTerm*font:              -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
!UXTerm*boldFont:          -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
UXTerm*utf8:              1
UXTerm*scrollBar:         false
UXTerm*rightScrollBar:    false

XTerm*termName:           xterm-256color
XTerm*geometry:           100x34
!XTerm*background:        #101012
!XTerm*foreground:        #E5E5E5
!XTerm*cursorColor:       #E5E5E5
!XTerm*font:               -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
!XTerm*boldFont:           -xos4-terminus-medium-r-normal-*-16-160-72-72-c-80-iso8859-1
XTerm*faceName:           DejaVu Sans Mono:book:size=11:antialias=true:autohint=true
XTerm*utf8:               1
XTerm*scrollBar:          false
XTerm*rightScrollBar:     false

! X-11 Apps------------------------------------------------------------------
xpdf*Background:    #CFC6BE
xpdf*Foreground:    #050505
xpdf*FontList:      -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
xpdf*initialZoom:   page
xpdf*urlCommand:    firefox %s
xpdf*textEncoding:  utf-8


! Xft settings ---------------------------------------------------------
Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
!Xft.autohint: 0

! special
*.foreground:   #EDEDED
*.background:   #0F0F0F
*.cursorColor:  #EDEDED

! black
*.color0:       #000000
*.color8:       #4d4d4d

! red
*.color1:       #cc0403
*.color9:       #800C0A

! green
*.color2:       #19cb00
*.color10:      #23fd00

! yellow
*.color3:       #cecb00
*.color11:      #fffd00

! blue
*.color4:       #001cd1
*.color12:      #0026ff

! magenta
*.color5:       #cb1ed1
*.color13:      #fd28ff

! cyan
*.color6:       #0dcdcd
*.color14:      #14ffff

! white
*.color7:       #e5e5e5
*.color15:      #ffffff

! URxvt -----------------------------------------------------------------------
URxvt*termName:          rxvt-256color
URxvt*scrollBar:         false
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt*depth:             32
URxvt.background:        [90]#0F0F0F

URxvt.font:              xft:Hack:Regular:size=11:antialias=true:autohint=true
URxvt.boldFont:          xft:Hack:Regular:size=11:antialias=true:autohint=true
!URxvt.font:              xft:IBM Plex Mono:medium:size=11:antialias=false
!URxvt.boldFont:          xft:IBM Plex Mono:medium:size=11:antialias=false
URxvt.letterSpace: 1

*antialias:              true
*autohint:               true

!URxvt.perl-ext-common: default,matcher,selection-to-clipboard
!URxvt.url-launcher: firefox
!URxvt.matcher.button: 1
URxvt.perl-ext-common:  matcher
URxvt.urlLauncher:      firefox
URxvt.tabbed.tabbar-fg: 14
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg:    13
URxvt.tabbed.tab-bg:    0


! XTerminals ------------------------------------------------------------
UXterm*termName:            xterm-256color
UXTerm*faceName:            xft:Hack:regular:size=11:antialias=false:autohint=true
UXTerm*utf8:                1
!UXTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!UXTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
UXTerm*background:         #171717
UXTerm*foreground:         #E5E5E5
UXTerm*cursorColor:        #E5E5E5
UXTerm*scrollBar:          false
UXTerm*rightScrollBar:     false

XTerm*termName:           xterm-256color
XTerm*background:         #171717
XTerm*foreground:         #E5E5E5
XTerm*cursorColor:        #E5E5E5
!XTerm*font:               -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!XTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
XTerm*faceName:           xft:Hack:regular:size=11:antialias=false:autohint=true
XTerm*utf8:               1
XTerm*scrollBar:          false
XTerm*rightScrollBar:     false

! X-11 Apps------------------------------------------------------------------
xpdf*Background:    #CFC6BE
xpdf*Foreground:    #050505
xpdf*FontList:      -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
xpdf*initialZoom:   page
xpdf*urlCommand:    firefox %s
xpdf*textEncoding:  utf-8

!source: https://wiki.archlinux.org/index.php/X_resources

UXTerm*boldFont:           -xos4-terminus-medium-r-normal-*-14-140-72-72-c-80-iso8859-1
!UXTerm*font:                xft:Perfect DOS VGA 437:style=Regular:size=12:antialias=false:autohint=true:hinting=slight
!UXTerm*boldFont:            xft:Perfect DOS VGA 437:style=Regular:size=12:antialias=false:autohint=true:hinting=slight
!UXTerm.faceName:           Ttyp0 Regular
!UXTerm*faceSize:           12
UXTerm*dynamicColors:      true
UXTerm*utf8:               2
UXTerm*eightBitInput:      true
UXTerm*saveLines:          2048
UXTerm*scrollTtyKeypress:  true
UXTerm*scrollTtyOutput:    false
UXTerm*scrollBar:          false
UXTerm*rightScrollBar:     false
UXTerm*jumpScroll:         true
UXTerm*multiScroll:        true

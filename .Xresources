xcalc*ti.button39.background: #555595
XCalc*Cursor:                 hand2
XCalc*ShapeStyle:             rectangle

dzen2.font:	-xos4-terminus-medium-r-normal-*-14-140-*-*-*-*-iso8859-1
!dzen2.font:	inconsolata:medium:pixelsize=14:antialias=true:hintstyle=slight
dzen2.foreground: #D2D2D2
dzen2.background: #191919

xclock*background: #363533
xclock*input: false
xclock*hourColor: #CCCCB8
xclock*minuteColor: #A3A393
xclock*secondColor: #D16356
xclock*majorColor: #CCCCCC
xclock*minorColor: #999999
xclock*width: 144
xclock*height: 144
xclock*update: 1
xclock*analog: true
xclock*padding: 4
xclock*twentyfour: true
!xclock*font: -xos4-terminus-medium-r-normal-*-14-140-*-*-*-*-iso8859-1
